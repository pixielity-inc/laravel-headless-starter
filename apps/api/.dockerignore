# ==============================================================================
# Laravel Headless Starter - .dockerignore
# ==============================================================================
# Files and directories to exclude from Docker build context
# Reduces image size and improves build performance
# ==============================================================================

# ------------------------------------------------------------------------------
# Version Control
# ------------------------------------------------------------------------------
.git
.gitignore
.gitattributes
.github

# ------------------------------------------------------------------------------
# IDE & Editor Files
# ------------------------------------------------------------------------------
.idea
.vscode
.fleet
.nova
.zed
*.swp
*.swo
*~

# ------------------------------------------------------------------------------
# Dependencies (will be installed in container)
# ------------------------------------------------------------------------------
vendor/
node_modules/
bower_components/

# ------------------------------------------------------------------------------
# Environment Files
# ------------------------------------------------------------------------------
.env
.env.*
!.env.example
env/.env
env/.env.*
!env/.env.example
!env/.env.production

# ------------------------------------------------------------------------------
# Build & Compiled Files
# ------------------------------------------------------------------------------
/public/build
/public/hot
/public/storage

# ------------------------------------------------------------------------------
# Storage (will be mounted as volumes)
# ------------------------------------------------------------------------------
storage/*.key
storage/pail
storage/app/*
!storage/app/.gitignore
!storage/app/public
!storage/app/private
storage/framework/cache/*
!storage/framework/cache/.gitignore
storage/framework/sessions/*
!storage/framework/sessions/.gitignore
storage/framework/testing/*
!storage/framework/testing/.gitignore
storage/framework/views/*
!storage/framework/views/.gitignore
storage/logs/*
!storage/logs/.gitignore

# ------------------------------------------------------------------------------
# Bootstrap Cache
# ------------------------------------------------------------------------------
bootstrap/cache/*
!bootstrap/cache/.gitignore

# ------------------------------------------------------------------------------
# Testing
# ------------------------------------------------------------------------------
tests/
.phpunit.result.cache
.phpunit.cache/
coverage/
phpunit.xml
.phpactor.json

# ------------------------------------------------------------------------------
# Documentation
# ------------------------------------------------------------------------------
*.md
!README.md
CONTRIBUTING.md
OCTANE.md
env/README.md

# ------------------------------------------------------------------------------
# Logs
# ------------------------------------------------------------------------------
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# ------------------------------------------------------------------------------
# Database
# ------------------------------------------------------------------------------
*.sqlite
*.sqlite-journal
database/*.sqlite
database/*.sqlite-journal

# ------------------------------------------------------------------------------
# Octane & Server Binaries
# ------------------------------------------------------------------------------
frankenphp
frankenphp-worker.php
rr
swoole
**/caddy

# ------------------------------------------------------------------------------
# Deployment Files
# ------------------------------------------------------------------------------
.deploy/
Homestead.json
Homestead.yaml
auth.json

# ------------------------------------------------------------------------------
# OS Generated Files
# ------------------------------------------------------------------------------
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
desktop.ini

# ------------------------------------------------------------------------------
# Temporary Files
# ------------------------------------------------------------------------------
*.tmp
*.temp
.cache/
.temp/

# ------------------------------------------------------------------------------
# CI/CD
# ------------------------------------------------------------------------------
.github/
.gitlab-ci.yml
.travis.yml
.circleci/

# ------------------------------------------------------------------------------
# Package Manager Files (optional - uncomment if needed)
# ------------------------------------------------------------------------------
# composer.lock
# package-lock.json
# yarn.lock
