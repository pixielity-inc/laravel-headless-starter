# Production-ready gitignore for Laravel applications
# ==============================================================================

# ------------------------------------------------------------------------------
# Environment & Configuration
# ------------------------------------------------------------------------------
.env
.env.*
!.env.example
env/.env
env/.env.*
!env/.env.example
!env/.env.local
!env/.env.production
!env/.env.testing
!env/README.md

# ------------------------------------------------------------------------------
# Dependencies
# ------------------------------------------------------------------------------
/vendor
/node_modules

# ------------------------------------------------------------------------------
# IDE & Editor Files
# ------------------------------------------------------------------------------
.idea/
.vscode/
.fleet/
.nova/
.zed/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# ------------------------------------------------------------------------------
# Laravel Specific
# ------------------------------------------------------------------------------
/bootstrap/cache/*
!bootstrap/cache/.gitignore
/storage/*.key
/storage/app/*
!storage/app/.gitignore
!storage/app/public
!storage/app/private
/storage/framework/cache/*
!storage/framework/cache/.gitignore
!storage/framework/cache/data
/storage/framework/cache/data/*
!storage/framework/cache/data/.gitignore
/storage/framework/sessions/*
!storage/framework/sessions/.gitignore
/storage/framework/testing/*
!storage/framework/testing/.gitignore
/storage/framework/views/*
!storage/framework/views/.gitignore
/storage/logs/*
!storage/logs/.gitignore
/storage/pail

# ------------------------------------------------------------------------------
# Build & Compiled Files
# ------------------------------------------------------------------------------
/public/build
/public/hot
/public/storage
npm-debug.log
yarn-error.log

# ------------------------------------------------------------------------------
# Testing
# ------------------------------------------------------------------------------
.phpunit.result.cache
.phpunit.cache/
coverage/
.phpactor.json

# ------------------------------------------------------------------------------
# Logs
# ------------------------------------------------------------------------------
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# ------------------------------------------------------------------------------
