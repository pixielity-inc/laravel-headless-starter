# ==============================================================================
# Laravel Environment Configuration
# ==============================================================================
# This file contains all environment variables used by the application.
# Copy this file to .env and update the values for your environment.
# ==============================================================================

# ------------------------------------------------------------------------------
# Application
# ------------------------------------------------------------------------------
# Core application settings including name, environment, and debug mode.
#
# APP_NAME              Application name displayed in UI and emails
# APP_ENV               Environment: local, staging, production
# APP_KEY               Encryption key (generate with: bin/artisan key:generate)
# APP_DEBUG             Enable debug mode (true for development, false for production)
# APP_URL               Base URL of the application
# APP_LOCALE            Default application locale
# APP_FALLBACK_LOCALE   Fallback locale when translation is missing
# APP_FAKER_LOCALE      Locale for Faker data generation
# APP_MAINTENANCE_DRIVER  Maintenance mode driver: file, cache
# APP_MAINTENANCE_STORE   Store for maintenance mode data
# APP_PREVIOUS_KEYS     Previous encryption keys for key rotation
# ------------------------------------------------------------------------------
APP_NAME="Laravel"
APP_ENV=local
APP_KEY=base64:UuO//pdCc6W6uh1Nvdv3dJR2pVa+Geq1FUE5oV3Z+6w=
APP_DEBUG=true
APP_URL=http://localhost
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US
APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=
# APP_PREVIOUS_KEYS=

# ------------------------------------------------------------------------------
# Database
# ------------------------------------------------------------------------------
# Database connection settings for primary and additional connections.
#
# DB_CONNECTION         Database driver: mysql, pgsql, sqlite, sqlsrv
# DB_HOST               Database server hostname
# DB_PORT               Database server port
# DB_DATABASE           Database name
# DB_USERNAME           Database username
# DB_PASSWORD           Database password
# DB_CHARSET            Character set (MySQL)
# DB_COLLATION          Collation (MySQL)
# DB_FOREIGN_KEYS       Enable foreign key constraints (SQLite)
# DB_SOCKET             Unix socket path (MySQL)
# DB_SSLMODE            SSL mode (PostgreSQL): disable, allow, prefer, require
# DB_ENCRYPT            Enable encryption (SQL Server)
# DB_TRUST_SERVER_CERTIFICATE  Trust server certificate (SQL Server)
# DB_URL                Full database connection URL
# DB_QUEUE              Database queue name
# DB_QUEUE_CONNECTION   Database connection for queue
# DB_QUEUE_TABLE        Table name for database queue
# DB_QUEUE_RETRY_AFTER  Retry after seconds for database queue
# DB_CACHE_CONNECTION   Database connection for cache
# DB_CACHE_TABLE        Table name for cache
# DB_CACHE_LOCK_CONNECTION  Database connection for cache locks
# DB_CACHE_LOCK_TABLE   Table name for cache locks
# ------------------------------------------------------------------------------
DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=
# DB_CHARSET=utf8mb4
# DB_COLLATION=utf8mb4_unicode_ci
# DB_FOREIGN_KEYS=true
# DB_SOCKET=
# DB_SSLMODE=prefer
# DB_ENCRYPT=
# DB_TRUST_SERVER_CERTIFICATE=
# DB_URL=
# DB_QUEUE=default
# DB_QUEUE_CONNECTION=
# DB_QUEUE_TABLE=jobs
# DB_QUEUE_RETRY_AFTER=90
# DB_CACHE_CONNECTION=
# DB_CACHE_TABLE=cache
# DB_CACHE_LOCK_CONNECTION=
# DB_CACHE_LOCK_TABLE=cache_locks
# MYSQL_ATTR_SSL_CA=

# ------------------------------------------------------------------------------
# Cache
# ------------------------------------------------------------------------------
# Cache driver and connection settings.
#
# CACHE_STORE           Cache driver: file, redis, memcached, dynamodb, database
# CACHE_PREFIX          Prefix for cache keys
# MEMCACHED_HOST        Memcached server hostname
# MEMCACHED_PORT        Memcached server port
# MEMCACHED_USERNAME    Memcached username (if authentication enabled)
# MEMCACHED_PASSWORD    Memcached password (if authentication enabled)
# MEMCACHED_PERSISTENT_ID  Persistent connection ID
# DYNAMODB_CACHE_TABLE  DynamoDB table name for cache
# DYNAMODB_ENDPOINT     DynamoDB endpoint URL
# ------------------------------------------------------------------------------
CACHE_STORE=database
# CACHE_PREFIX=
# MEMCACHED_HOST=127.0.0.1
# MEMCACHED_PORT=11211
# MEMCACHED_USERNAME=
# MEMCACHED_PASSWORD=
# MEMCACHED_PERSISTENT_ID=
# DYNAMODB_CACHE_TABLE=cache
# DYNAMODB_ENDPOINT=

# ------------------------------------------------------------------------------
# Queue
# ------------------------------------------------------------------------------
# Queue driver and connection settings for background job processing.
#
# QUEUE_CONNECTION      Queue driver: sync, database, redis, sqs, beanstalkd
# QUEUE_FAILED_DRIVER   Failed jobs driver: database, dynamodb
# SQS_PREFIX            SQS queue URL prefix
# SQS_QUEUE             SQS queue name
# SQS_SUFFIX            SQS queue URL suffix
# BEANSTALKD_QUEUE_HOST  Beanstalkd server hostname
# BEANSTALKD_QUEUE      Beanstalkd queue name
# BEANSTALKD_QUEUE_RETRY_AFTER  Retry after seconds
# ------------------------------------------------------------------------------
QUEUE_CONNECTION=database
# QUEUE_FAILED_DRIVER=database
# SQS_PREFIX=
# SQS_QUEUE=
# SQS_SUFFIX=
# BEANSTALKD_QUEUE_HOST=localhost
# BEANSTALKD_QUEUE=default
# BEANSTALKD_QUEUE_RETRY_AFTER=90

# ------------------------------------------------------------------------------
# Redis
# ------------------------------------------------------------------------------
# Redis connection settings for cache, queue, and session storage.
#
# REDIS_CLIENT          Redis client: phpredis, predis
# REDIS_HOST            Redis server hostname
# REDIS_PORT            Redis server port
# REDIS_PASSWORD        Redis password
# REDIS_USERNAME        Redis username (Redis 6+)
# REDIS_DB              Redis database number
# REDIS_PREFIX          Prefix for Redis keys
# REDIS_CLUSTER         Enable Redis cluster mode
# REDIS_TIMEOUT         Connection timeout in seconds
# REDIS_PERSISTENT      Enable persistent connections
# REDIS_URL             Full Redis connection URL
# REDIS_CACHE_CONNECTION  Redis connection for cache
# REDIS_CACHE_DB        Redis database for cache
# REDIS_CACHE_LOCK_CONNECTION  Redis connection for cache locks
# REDIS_QUEUE_CONNECTION  Redis connection for queue
# REDIS_QUEUE           Redis queue name
# REDIS_QUEUE_RETRY_AFTER  Retry after seconds
# REDIS_MAX_RETRIES     Maximum retry attempts
# REDIS_BACKOFF_ALGORITHM  Backoff algorithm: exponential, linear
# REDIS_BACKOFF_BASE    Base backoff time in milliseconds
# REDIS_BACKOFF_CAP     Maximum backoff time in milliseconds
# ------------------------------------------------------------------------------
# REDIS_CLIENT=phpredis
# REDIS_HOST=127.0.0.1
# REDIS_PORT=6379
# REDIS_PASSWORD=
# REDIS_USERNAME=
# REDIS_DB=0
# REDIS_PREFIX=laravel_
# REDIS_CLUSTER=false
# REDIS_TIMEOUT=0
# REDIS_PERSISTENT=false
# REDIS_URL=
# REDIS_CACHE_CONNECTION=cache
# REDIS_CACHE_DB=1
# REDIS_CACHE_LOCK_CONNECTION=
# REDIS_QUEUE_CONNECTION=default
# REDIS_QUEUE=default
# REDIS_QUEUE_RETRY_AFTER=90
# REDIS_MAX_RETRIES=3
# REDIS_BACKOFF_ALGORITHM=exponential
# REDIS_BACKOFF_BASE=100
# REDIS_BACKOFF_CAP=10000

# ------------------------------------------------------------------------------
# Session
# ------------------------------------------------------------------------------
# Session driver and cookie settings.
#
# SESSION_DRIVER        Session driver: file, cookie, database, redis, memcached
# SESSION_LIFETIME      Session lifetime in minutes
# SESSION_ENCRYPT       Encrypt session data
# SESSION_PATH          Cookie path
# SESSION_DOMAIN        Cookie domain
# SESSION_SECURE_COOKIE  Send cookie only over HTTPS
# SESSION_HTTP_ONLY     Make cookie accessible only through HTTP protocol
# SESSION_SAME_SITE     SameSite cookie attribute: lax, strict, none
# SESSION_PARTITIONED_COOKIE  Enable partitioned cookies
# SESSION_EXPIRE_ON_CLOSE  Expire session when browser closes
# SESSION_CONNECTION    Database/Redis connection for sessions
# SESSION_STORE         Cache store for sessions
# SESSION_TABLE         Database table for sessions
# ------------------------------------------------------------------------------
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=false
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=lax
SESSION_PARTITIONED_COOKIE=false
# SESSION_EXPIRE_ON_CLOSE=false
# SESSION_CONNECTION=
# SESSION_STORE=
# SESSION_TABLE=sessions

# ------------------------------------------------------------------------------
# Broadcasting
# ------------------------------------------------------------------------------
# Real-time event broadcasting settings.
#
# BROADCAST_CONNECTION  Broadcast driver: log, pusher, ably, redis, reverb
# PUSHER_APP_ID         Pusher application ID
# PUSHER_APP_KEY        Pusher application key
# PUSHER_APP_SECRET     Pusher application secret
# PUSHER_APP_CLUSTER    Pusher cluster
# PUSHER_HOST           Pusher host (for self-hosted)
# PUSHER_PORT           Pusher port
# PUSHER_SCHEME         Pusher scheme: http, https
# ABLY_KEY              Ably API key
# ------------------------------------------------------------------------------
BROADCAST_CONNECTION=log
# PUSHER_APP_ID=
# PUSHER_APP_KEY=
# PUSHER_APP_SECRET=
# PUSHER_APP_CLUSTER=mt1
# PUSHER_HOST=
# PUSHER_PORT=443
# PUSHER_SCHEME=https
# ABLY_KEY=

# ------------------------------------------------------------------------------
# Reverb (WebSocket Server)
# ------------------------------------------------------------------------------
# Laravel Reverb WebSocket server configuration.
#
# REVERB_SERVER         Reverb server: reverb
# REVERB_APP_ID         Application ID
# REVERB_APP_KEY        Application key
# REVERB_APP_SECRET     Application secret
# REVERB_HOST           Server hostname
# REVERB_PORT           Server port
# REVERB_SCHEME         Scheme: http, https
# REVERB_SERVER_HOST    Server bind address
# REVERB_SERVER_PORT    Server bind port
# REVERB_SERVER_PATH    Server path
# REVERB_APP_PING_INTERVAL  Ping interval in seconds
# REVERB_APP_ACTIVITY_TIMEOUT  Activity timeout in seconds
# REVERB_APP_MAX_CONNECTIONS  Maximum connections per app
# REVERB_APP_MAX_MESSAGE_SIZE  Maximum message size in KB
# REVERB_MAX_REQUEST_SIZE  Maximum request size in KB
# REVERB_SCALING_ENABLED  Enable horizontal scaling
# REVERB_SCALING_CHANNEL  Redis channel for scaling
# REVERB_PULSE_INGEST_INTERVAL  Pulse ingest interval
# REVERB_TELESCOPE_INGEST_INTERVAL  Telescope ingest interval
# ------------------------------------------------------------------------------
# REVERB_SERVER=reverb
# REVERB_APP_ID=
# REVERB_APP_KEY=
# REVERB_APP_SECRET=
# REVERB_HOST=localhost
# REVERB_PORT=8080
# REVERB_SCHEME=http
# REVERB_SERVER_HOST=0.0.0.0
# REVERB_SERVER_PORT=8080
# REVERB_SERVER_PATH=/
# REVERB_APP_PING_INTERVAL=30
# REVERB_APP_ACTIVITY_TIMEOUT=30
# REVERB_APP_MAX_CONNECTIONS=
# REVERB_APP_MAX_MESSAGE_SIZE=10000
# REVERB_MAX_REQUEST_SIZE=10000
# REVERB_SCALING_ENABLED=false
# REVERB_SCALING_CHANNEL=
# REVERB_PULSE_INGEST_INTERVAL=
# REVERB_TELESCOPE_INGEST_INTERVAL=

# ------------------------------------------------------------------------------
# Filesystem
# ------------------------------------------------------------------------------
# File storage driver and cloud storage settings.
#
# FILESYSTEM_DISK       Default filesystem disk: local, public, s3
# AWS_ACCESS_KEY_ID     AWS access key ID
# AWS_SECRET_ACCESS_KEY  AWS secret access key
# AWS_DEFAULT_REGION    AWS region
# AWS_BUCKET            S3 bucket name
# AWS_URL               S3 custom URL
# AWS_ENDPOINT          S3 endpoint (for S3-compatible services)
# AWS_USE_PATH_STYLE_ENDPOINT  Use path-style endpoint
# ------------------------------------------------------------------------------
FILESYSTEM_DISK=local
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1
# AWS_BUCKET=
# AWS_URL=
# AWS_ENDPOINT=
# AWS_USE_PATH_STYLE_ENDPOINT=false

# ------------------------------------------------------------------------------
# Mail
# ------------------------------------------------------------------------------
# Email sending configuration.
#
# MAIL_MAILER           Mail driver: smtp, sendmail, mailgun, ses, postmark, log
# MAIL_HOST             SMTP server hostname
# MAIL_PORT             SMTP server port
# MAIL_USERNAME         SMTP username
# MAIL_PASSWORD         SMTP password
# MAIL_ENCRYPTION       Encryption: tls, ssl
# MAIL_FROM_ADDRESS     Default from email address
# MAIL_FROM_NAME        Default from name
# MAIL_URL              Full mail connection URL
# MAIL_SCHEME           Mail scheme
# MAIL_EHLO_DOMAIN      EHLO domain
# MAIL_SENDMAIL_PATH    Sendmail binary path
# MAIL_LOG_CHANNEL      Log channel for mail driver
# POSTMARK_MESSAGE_STREAM_ID  Postmark message stream ID
# ------------------------------------------------------------------------------
MAIL_MAILER=log
# MAIL_HOST=127.0.0.1
# MAIL_PORT=2525
# MAIL_USERNAME=
# MAIL_PASSWORD=
# MAIL_ENCRYPTION=
# MAIL_FROM_ADDRESS="hello@example.com"
# MAIL_FROM_NAME="${APP_NAME}"
# MAIL_URL=
# MAIL_SCHEME=
# MAIL_EHLO_DOMAIN=
# MAIL_SENDMAIL_PATH=/usr/sbin/sendmail -bs
# MAIL_LOG_CHANNEL=
# POSTMARK_MESSAGE_STREAM_ID=

# ------------------------------------------------------------------------------
# Logging
# ------------------------------------------------------------------------------
# Application logging configuration.
#
# LOG_CHANNEL           Log channel: stack, single, daily, slack, syslog, stderr
# LOG_STACK             Channels for stack driver
# LOG_LEVEL             Minimum log level: debug, info, notice, warning, error
# LOG_DEPRECATIONS_CHANNEL  Channel for deprecation warnings
# LOG_DEPRECATIONS_TRACE  Include stack trace for deprecations
# LOG_DAILY_DAYS        Days to keep daily logs
# LOG_SLACK_WEBHOOK_URL  Slack webhook URL
# LOG_SLACK_USERNAME    Slack bot username
# LOG_SLACK_EMOJI       Slack bot emoji
# LOG_PAPERTRAIL_URL    Papertrail URL
# LOG_PAPERTRAIL_PORT   Papertrail port
# LOG_PAPERTRAIL_HANDLER  Papertrail handler class
# LOG_SYSLOG_FACILITY   Syslog facility
# LOG_STDERR_FORMATTER  Stderr formatter class
# ------------------------------------------------------------------------------
LOG_CHANNEL=stack
# LOG_STACK=single
LOG_LEVEL=debug
# LOG_DEPRECATIONS_CHANNEL=
# LOG_DEPRECATIONS_TRACE=false
# LOG_DAILY_DAYS=14
# LOG_SLACK_WEBHOOK_URL=
# LOG_SLACK_USERNAME=
# LOG_SLACK_EMOJI=
# LOG_PAPERTRAIL_URL=
# LOG_PAPERTRAIL_PORT=
# LOG_PAPERTRAIL_HANDLER=
# LOG_SYSLOG_FACILITY=
# LOG_STDERR_FORMATTER=

# ------------------------------------------------------------------------------
# Authentication
# ------------------------------------------------------------------------------
# Authentication guards and providers.
#
# AUTH_GUARD            Default authentication guard
# AUTH_MODEL            User model class
# AUTH_PASSWORD_BROKER  Password reset broker
# AUTH_PASSWORD_RESET_TOKEN_TABLE  Password reset tokens table
# AUTH_PASSWORD_TIMEOUT  Password reset timeout in seconds
# ------------------------------------------------------------------------------
# AUTH_GUARD=web
# AUTH_MODEL=App\Models\User
# AUTH_PASSWORD_BROKER=users
# AUTH_PASSWORD_RESET_TOKEN_TABLE=password_reset_tokens
# AUTH_PASSWORD_TIMEOUT=3600

# ------------------------------------------------------------------------------
# Laravel Octane
# ------------------------------------------------------------------------------
# High-performance application server settings.
#
# OCTANE_SERVER         Server: swoole, roadrunner, frankenphp
# OCTANE_HTTPS          Enable HTTPS
# OCTANE_WORKERS        Number of worker processes
# OCTANE_TASK_WORKERS   Number of task worker processes
# OCTANE_MAX_REQUESTS   Maximum requests per worker before restart
# ------------------------------------------------------------------------------
# OCTANE_SERVER=swoole
# OCTANE_HTTPS=false
# OCTANE_WORKERS=auto
# OCTANE_TASK_WORKERS=auto
# OCTANE_MAX_REQUESTS=500

# ------------------------------------------------------------------------------
# Laravel Boost
# ------------------------------------------------------------------------------
# Development productivity tools and watchers.
#
# BOOST_ENABLED         Enable Boost features
# BOOST_PHP_EXECUTABLE_PATH  Path to PHP executable
# BOOST_COMPOSER_EXECUTABLE_PATH  Path to Composer executable
# BOOST_NPM_EXECUTABLE_PATH  Path to NPM executable
# BOOST_VENDOR_BIN_EXECUTABLE_PATH  Path to vendor/bin directory
# BOOST_BROWSER_LOGS_WATCHER  Enable browser logs watcher
# ------------------------------------------------------------------------------
# BOOST_ENABLED=true
# BOOST_PHP_EXECUTABLE_PATH=
# BOOST_COMPOSER_EXECUTABLE_PATH=
# BOOST_NPM_EXECUTABLE_PATH=
# BOOST_VENDOR_BIN_EXECUTABLE_PATH=
# BOOST_BROWSER_LOGS_WATCHER=true
